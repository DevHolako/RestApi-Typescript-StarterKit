{
  "compilerOptions": {
    "target": "es2016",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "baseUrl": "./src",
    "paths": {
      "@/controllers/*": [
        "controllers/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/middlewares/*": [
        "middlewares/*"
      ],
      "@/models/*": [
        "models/*"
      ],
      "@/routes/*": [
        "routes/*"
      ],
      "@/intrefaces/*": [
        "intrefaces/*"
      ]
    },
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}